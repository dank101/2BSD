install:
	mkdir /usr/lib/me
	cp acm.me /usr/lib/me/acm.me
	cp chars.me /usr/lib/me/chars.me
	cp deltext.me /usr/lib/me/deltext.me
	cp eqn.me /usr/lib/me/eqn.me
	cp float.me /usr/lib/me/float.me
	cp footnote.me /usr/lib/me/footnote.me
	cp index.me /usr/lib/me/index.me
	cp local.me /usr/lib/me/local.me
	cp null.me /usr/lib/me/null.me
	cp sh.me /usr/lib/me/sh.me
	cp tbl.me /usr/lib/me/tbl.me
	cp thesis.me /usr/lib/me/thesis.me
	cp tmac.e /usr/lib/tmac/tmac.e
	ed - /usr/lib/me/acm.me <strip-me.ed
	ed - /usr/lib/me/chars.me <strip-me.ed
	ed - /usr/lib/me/deltext.me <strip-me.ed
	ed - /usr/lib/me/eqn.me <strip-me.ed
	ed - /usr/lib/me/float.me <strip-me.ed
	ed - /usr/lib/me/footnote.me <strip-me.ed
	ed - /usr/lib/me/index.me <strip-me.ed
	ed - /usr/lib/me/local.me <strip-me.ed
	ed - /usr/lib/me/null.me <strip-me.ed
	ed - /usr/lib/me/sh.me <strip-me.ed
	ed - /usr/lib/me/tbl.me <strip-me.ed
	ed - /usr/lib/me/thesis.me <strip-me.ed
	ed - /usr/lib/tmac/tmac.e <strip-me.ed
	cp revisions /usr/lib/me

clean:
